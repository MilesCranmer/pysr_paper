@article{lyonInteractiveLanguageLearning2012,
  title = {Interactive {{Language Learning}} by {{Robots}}: {{The Transition}} from {{Babbling}} to {{Word Forms}}},
  author = {Lyon, Caroline and Nehaniv, Chrystopher L. and Saunders, Joe},
