@article{ziatdinovHypothesisLearningAutomated2022,
  title = {Hypothesis {{Learning}} in {{Automated Experiment}}: {{Application}} to {{Combinatorial Materials Libraries}}},
  shorttitle = {Hypothesis {{Learning}} in {{Automated Experiment}}},
  author = {Ziatdinov, Maxim A. and Liu, Yongtao and Morozovska, Anna N. and Eliseev, Eugene A. and Zhang, Xiaohang and Takeuchi, Ichiro and Kalinin, Sergei V.},
  year = {2022},
  journal = {Advanced Materials},
  volume = {34},
