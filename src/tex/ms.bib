@article{arevaloStabilityConstrainedCharacterization2022,
  title = {Stability {{Constrained Characterization}} of the 23 {{Myr Old V1298 Tau System}}: {{Do Young Planets Form}} in {{Mean Motion Resonance Chains}}?},
