@article{ghoshGaMPENMachinelearningFramework2022,
  title = {{{GaMPEN}}: {{A Machine-learning Framework}} for {{Estimating Bayesian Posteriors}} of {{Galaxy Morphological Parameters}}},
  shorttitle = {{{GaMPEN}}},
  author = {Ghosh, Aritra and Urry, C. Megan and Rau, Amrit and {Perreault-Levasseur}, Laurence and Cranmer, Miles and Schawinski, Kevin and Stark, Dominic and Tian, Chuan and Ofman, Ryan and Ananna, Tonima Tasnim and Auge, Connor and Cappelluti, Nico and Sanders, David B. and Treister, Ezequiel},
  year = {2022},
