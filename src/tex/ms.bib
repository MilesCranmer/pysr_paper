@article{breuvalInfluenceMetallicityLeavitt2021,
  title = {The {{Influence}} of {{Metallicity}} on the {{Leavitt Law}} from {{Geometrical Distances}} of {{Milky Way}} and {{Magellanic Cloud Cepheids}}},
  author = {Breuval, Louise and Kervella, Pierre and Wielgórski, Piotr and Gieren, Wolfgang and Graczyk, Dariusz and Trahin, Boris and Pietrzyński, Grzegorz and Arenou, Frédéric and Javanmardi, Behnam and Zgirski, Bartłomiej},
