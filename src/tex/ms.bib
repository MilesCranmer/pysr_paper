  shorttitle = {Reconciling {{Dwarf Galaxies}} with {{ΛCDM Cosmology}}},
  author = {Wetzel, Andrew R. and Hopkins, Philip F. and Kim, Ji-hoon and {Faucher-Giguère}, Claude-André and Kereš, Dušan and Quataert, Eliot},
