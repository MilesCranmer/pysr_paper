@article{huttonAstronomicalImageCoaddition2022,
  title = {Astronomical {{Image Coaddition}} with {{Bundle-Adjusting Radiance Fields}}},
  author = {Hutton, Harlan and Palegar, Harshitha and Eubank, Jenna and Ho, Shirley and Cranmer, Miles and Melchior, Peter},
  year = {2022},
