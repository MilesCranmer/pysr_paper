@article{anisimovaMitigatingRadiationDamage2017,
  title = {Mitigating Radiation Damage of Single Photon Detectors for Space Applications},
  author = {Anisimova, Elena and Higgins, Brendon L. and Bourgoin, Jean-Philippe and Cranmer, Miles and Choi, Eric and Hudson, Danya and Piche, Louis P. and Scott, Alan and Makarov, Vadim and Jennewein, Thomas},
