@article{canutoTURBULENCEASTROPHYSICSStars1998,
  title = {{{TURBULENCE IN ASTROPHYSICS}}: {{Stars}}},
  shorttitle = {{{TURBULENCE IN ASTROPHYSICS}}},
  author = {Canuto, V. M. and {Christensen-Dalsgaard}, J.},
