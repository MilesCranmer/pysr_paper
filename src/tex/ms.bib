  shorttitle = {The {{PAndAS Field}} of {{Streams}}},
  author = {Martin, Nicolas F. and Ibata, Rodrigo A. and Rich, R. Michael and Collins, Michelle L. M. and Fardal, Mark A. and Irwin, Michael J. and Lewis, Geraint F. and McConnachie, Alan W. and Babul, Arif and Bate, Nicholas F. and Chapman, Scott C. and Conn, Anthony R. and Crnojević, Denija and Ferguson, Annette M. N. and Mackey, A. Dougal and Navarro, Julio F. and Peñarrubia, Jorge and Tanvir, Nial T. and {Valls-Gabaud}, David},
